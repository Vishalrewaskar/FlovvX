    "build": "npm run build:css",
    "start": "npm run build:css && npx serve ."
  },
  "devDependencies": {
    "autoprefixer": "^10.4.18",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1"
  }
}